     "execution_count": 219,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "freq_of_B = np.sum(pop)\n",
    "freq_of_A = np.size(pop) - freq_of_B\n",
    "freq_of_A, freq_of_B"
   ]
  },
